CREATE TABLE DF1 (ID INT);
INSERT INTO DF1
VALUES (1), (1), (2), (2), (3), (3), (3), (4), (5), (NULL), (NULL);

SELECT *
FROM DF1;

CREATE TABLE DF2 (ID INT);
INSERT INTO DF2
VALUES (1), (3), (4), (NULL);
INSERT INTO DF2 VALUES (3);

SELECT * 
FROM DF2;

SELECT *
FROM DF1
INNER JOIN DF2
ON DF1.ID = DF2.ID;

SELECT *
FROM DF1
LEFT JOIN DF2
ON DF1.ID = DF2.ID;

SELECT *
FROM DF1
RIGHT JOIN DF2
ON DF1.ID = DF2.ID;

SELECT *
FROM DF1
FULL OUTER JOIN DF2
ON DF1.ID = DF2.ID;